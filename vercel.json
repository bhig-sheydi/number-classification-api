{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/classify",
        "dest": "server.js",
        "methods": ["GET"]
      },
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ]
  }